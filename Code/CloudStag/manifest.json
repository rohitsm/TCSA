{
  "manifest_version": 2,

  "name": "Cloud Stag",
  "description": "This extension works with cloudstag.me",
  "version": "1.0",

  "background": {
    "scripts": ["static/js/bg.js"]
  },

  "optional_permissions": [ "tabs", "https://cloudstag.me" ],

  "browser_action": {
    "default_icon": "icon.png"
  }
}